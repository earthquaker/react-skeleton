{
  "name": "livereactload",
  "version": "2.1.0",
  "description": "Live code editing with Browserify and React",
  "author": {
    "name": "Matti Lankinen",
    "email": "m.lankinen@iki.fi",
    "url": "https://github.com/milankinen"
  },
  "license": "MIT",
